function eliminarFilasEnBlanco(spreadsheetId, hojaNombre) {
  // Usar el ID de la URL
  const spreadsheet = SpreadsheetApp.openById(spreadsheetId);
  const hoja = spreadsheet.getSheetByName(hojaNombre);
  
  if (!hoja) {
    return "Error: No se encontró la hoja: " + hojaNombre;
  }

  const ultimaFila = hoja.getLastRow();
  const ultimaColumna = hoja.getLastColumn();
  
  if (ultimaFila < 2) return "No hay datos para limpiar";
  
  const rango = hoja.getRange(2, 1, ultimaFila - 1, ultimaColumna);
  const datos = rango.getValues();
  const filasAEliminar = [];
  
  for (let i = 0; i < datos.length; i++) {
    const fila = datos[i];
    const vacía = fila.every(celda => !celda || celda.toString().trim() === "");
    if (vacía) filasAEliminar.push(i + 2);
  }
  
  for (let j = filasAEliminar.length - 1; j >= 0; j--) {
    hoja.deleteRow(filasAEliminar[j]);
  }
  
  return "Filas vacías eliminadas: " + filasAEliminar.length;
}
